{
  "name": "minimalcss-heroku",
  "version": "1.0.0",
  "description": "Test out minimalcss in a web app",
  "engines": {
    "node": "9.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "ui/node_modules"
  ],
  "dependencies": {
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "express": "4.16.2",
    "lru-cache": "4.1.1",
    "minimalcss": "0.7.2",
    "performance-now": "2.1.0",
    "prettier": "1.10.2",
    "response-time": "2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peterbe/minimalcss-heroku.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react",
    "minimalcss",
    "css"
  ],
  "license": "MIT",
  "devDependencies": {}
}
